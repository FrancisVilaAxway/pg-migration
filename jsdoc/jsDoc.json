{
  "source": {
    "include": [
      "lib"
    ]
  },
  "templates": {
    "default": {
      "layoutFile": "jsdoc/layout.html",
      "staticFiles": {
        "include": ["./jsdoc/style.css"]
      }
    }
  },
  "opts": {
    "recurse": true,
    "destination": "./docs",
    "encoding": "utf8"
  },
  "plugins": [
    ["plugins/markdown"]
  ]
}